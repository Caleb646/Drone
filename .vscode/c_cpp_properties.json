{
    "configurations": [
        {
            "name": "Linux WSL DEBUG",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/CM4",
                "${workspaceFolder}/CM7",
                "${workspaceFolder}/Drivers",
                "${workspaceFolder}/Middlewares"
            ],
            "defines": [
                "DEBUG",
                "CORE_CM4",
                "CORE_CM7",
                "USE_HAL_DRIVER",
                "STM32H747xx",
                "USE_PWR_DIRECT_SMPS_SUPPLY"
        ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "gcc-arm",
            "configurationProvider": "none"
        }
    ],
    "version": 4
}